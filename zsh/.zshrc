
# Source general environment variables
source ~/.zshrc.d/environment.zsh

# Source environment variables for programming languages
source ~/.zshrc.d/languages.zsh

# Start gamon
source ~/.zshrc.d/gamon.zsh

# Source functions
source ~/.zshrc.d/functions.zsh

# Source aliases
source ~/.zshrc.d/aliases.zsh

# Enable shell completion
source ~/.zshrc.d/completion.zsh

# Start starship terminal
source ~/.zshrc.d/starship.zsh

# Initialise sdkman
source ~/.zshrc.d/sdkman.zsh

# Enable zsh plugins
# (must go last)
source ~/.zshrc.d/zsh-plugins.zsh
